const arrr = ["luffy", "zoro", "sanji", "luffy", "nami", "zoro", "luffy"];
const ans = arrr.reduce((acc, curr) => {
  if (acc.hasOwnProperty(curr)) acc[curr]++;
  else acc[curr] = 1;
  return acc;
// }, {});
}, []);
console.log(ans); //{ luffy: 3, zoro: 2, sanji: 1, nami: 1 }
// [ luffy: 3, zoro: 2, sanji: 1, nami: 1 ]
